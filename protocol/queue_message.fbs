namespace flatbuf;

/// The format for serializing a queue message.
table QueueMessage {
    /// The unique identifier generated by the client API.
    unique_id: [ubyte];

    /// The pipelines to process.
    pipelines: [Pipeline];

    /// The API caller's payload of the message.
    payload: [ubyte];
}

table Pipeline {
    /// The name of the pipeline
    name: string;
}
